URxvt.geometry: 92x10
URxvt.font: xft:lemon:size=8
URxvt.boldFont: xft:lemon-bold:size=8
URxvt.internalBorder: 7 
URxvt.scrollBar: none
URxvt.letterSpace: 0
URxvt.iso14755: False
URxvt.keysym.Shift-Control-C: perl:clipboard:copy
URxvt.keysym.Shift-Control-V: perl:clipboard:paste
URxvt.perl-ext-common: default,matcher
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1

rofi.clor-enable:	true
rofi.color-normal:	#1e1e1e, #c3c3c3, #1e1e1e, #cf6a4c, #1e1e1e
rofi.color-window:	#1e1e1e, #cf6a4c, #cf6a4c
rofi.separator-styled:	solid

! special
*.foreground:   #a7a7a7
*.background:   #1e1e1e
*.cursorColor:  #a7a7a7

! black
*.color0:       #1e1e1e
*.color8:       #5f5a60

! red
*.color1:       #cf6a4c
*.color9:       #cf6a4c

! green
*.color2:       #8f9d6a
*.color10:      #8f9d6a

! yellow
*.color3:       #f9ee98
*.color11:      #f9ee98

! blue
*.color4:       #7587a6
*.color12:      #7587a6

! magenta
*.color5:       #9b859d
*.color13:      #9b859d

! cyan
*.color6:       #afc4db
*.color14:      #afc4db

! white
*.color7:       #a7a7a7
*.color15:      #ffffff

